Algoritmo "EXPRESSÃO MAIS LONGA"

Var
      nome : caractere
      idade : inteiro
      salario : real

Inicio

      nome <- "Fabielli"
      idade <- 22
      salario <- 2.200
      
      escreva("A mulher", nome, "que tem",  idade, "anos", "ganha", salario:8:3, "reais")


Fimalgoritmo